<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users List</title>
</head>
<body>
    <h1>List of Users</h1>
    <ul>
        <% users.forEach(user => { %>
            <li>
                <strong>Name:</strong> <%= user.name %> |
                <strong>Age:</strong> <%= user.age %> |
                <strong>Email:</strong> <%= user.email %>
            </li>
        <% }) %>
    </ul>
</body>
</html>
